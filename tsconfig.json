{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
     "outDir": "./dist",
     "rootDir": "./",
    "strict": true,
    "strictPropertyInitialization": false,
    "baseUrl": "./api",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
